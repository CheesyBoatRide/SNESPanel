{
    "apps": [
        {
            "name": "SNI",
            "args": [],
            "cmd": "./sni.exe",
            "working_directory": "./sni-v0.0.96-windows-amd64",
            "env": {
                "SNI_USB2SNES_LISTEN_ADDRS": "0.0.0.0:23074,0.0.0.0:8080"
            },
            "launch_on_start": true
        },
        {
            "name": "File Explorer",
            "cmd": "./sni-v0.0.96-windows-amd64/manage_files.exe"
        },
        {
            "name": "Tracker",
            "cmd": "./EmoTracker/EmoTracker.exe"
        },
        {
            "name": "Timer",
            "cmd": "./LiveSplit_1.8.21/LiveSplit.exe",
            "args": [
                "-l", "./splits/timer.lsl", "-s", "./splits/timer.lss"
            ]
        },
        {
            "name": "Archipelago SNI Client",
            "cmd": "./Archipelago/ArchipelagoSNIClient.exe"
        }
    ],
    "applets": [
        {
            "name": "Super Metroid Debug",
            "html": "./src/applets/super_metroid_debug/super_metroid_debug.html",
            "width": 680,
            "height": 940
        },
        {
            "name": "Mario Death Counter",
            "html": "./src/applets/mario_death_counter/mario_death_counter.html"
        },
        {
            "name": "Funtoon",
            "html": "https://funtoon.party/funtoon2snes/",
            "embedded": true
        },
        {
            "name": "Subversion Tracker",
            "html": "https://cout.github.io/sm_subversion_tracker/",
            "width": 800,
            "height": 600,
            "dev_tools": true
        }
    ],
    "expanded_groups": [
        "Custom Apps",
        "SNES Controls",
        "Subversion Tracker",
        "Applets"
    ],
    "usb2snes": {
        "usb2snes_address": "ws://localhost:23074/",
        "hotkeys": {
            "reset": "",
            "menu" : ""
        }
    }
}